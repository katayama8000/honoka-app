{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APP_ENV": "production",
        "APP_ENV": "production"
      },
      "autoIncrement": "true"
    },
    "development": {
      "env": {
        "EXPO_PUBLIC_APP_ENV": "development",
        "APP_ENV": "development"
      },
      "extends": "production"
    },
    "local": {
      "developmentClient": true,
      "env": {
        "EXPO_PUBLIC_APP_ENV": "local",
        "APP_ENV": "local"
      },
      "extends": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
