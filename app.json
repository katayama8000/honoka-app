{
  "expo": {
    "name": "household-account-book",
    "slug": "household-account-book",
    "orientation": "portrait",
    "icon": "./assets/images/moufu.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "version": "2.4.0",
    "splash": {
      "image": "./assets/images/moufu.png",
      "resizeMode": "contain",
      "backgroundColor": "#336666"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.katayama9000.householdaccountbook.dev"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/moufu.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.katayama9000.householdaccountbook",
      "versionCode": 23
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "018a6711-ac8c-41b8-830c-279089162afa"
      }
    },
    "owner": "katayama9000"
  }
}
